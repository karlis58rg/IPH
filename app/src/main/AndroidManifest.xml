<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.ssp.iph">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IPH"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".principal.ui.activitys.Principal"
            android:label="@string/title_activity_principal"
            android:theme="@style/Theme.IPH.NoActionBar"
            android:screenOrientation="portrait">
        </activity>

        <activity android:name=".Splash" android:screenOrientation="portrait" android:theme="@style/Theme.IPH.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".administrativo.ui.activitys.Iph_Administrativo" android:screenOrientation="portrait"/>

        <activity android:name=".administrativo.ui.activitys.Iph_Administrativo_Up" android:screenOrientation="portrait"></activity>

        <activity android:name=".Login" android:theme="@style/Theme.IPH.NoActionBar" android:screenOrientation="portrait"></activity>
    </application>

</manifest>